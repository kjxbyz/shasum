{
  "name": "ts-shasum",
  "version": "1.0.1",
  "description": "Compute and check SHA256 message digest.",
  "keywords": [
    "sha256sum",
    "shasum"
  ],
  "homepage": "https://github.com/kjxbyz/shasum",
  "bugs": "https://github.com/kjxbyz/shasum/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjxbyz/shasum.git"
  },
  "license": "MIT",
  "author": "kjxbyz <kjxbyz888@outlook.com> (https://github.com/kjxbyz)",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.d.mts",
        "default": "./dist/main.mjs"
      },
      "require": {
        "types": "./dist/main.d.ts",
        "default": "./dist/main.js"
      }
    },
    "./version": {
      "import": {
        "types": "./dist/version.d.mts",
        "default": "./dist/version.mjs"
      },
      "require": {
        "types": "./dist/version.d.ts",
        "default": "./dist/version.js"
      }
    }
  },
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "bin": {
    "shasum": "bin/shasum.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "test": "vitest run"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@types/node": "18.19.48",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "sort-package-json": "^2.10.1",
    "tsup": "^8.2.4",
    "typescript": "^4.9.5",
    "vitest": "^2.0.5"
  },
  "peerDependencies": {
    "glob": "^11.0.0"
  },
  "packageManager": "pnpm@9.9.0",
  "publishConfig": {
    "access": "public"
  }
}
