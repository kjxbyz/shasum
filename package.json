{
  "name": "ts-shasum",
  "version": "1.0.0-pre.1",
  "description": "Compute and check SHA256 message digest.",
  "keywords": [
    "sha256sum",
    "shasum"
  ],
  "homepage": "https://github.com/kjxbyz/shasum",
  "bugs": "https://github.com/kjxbyz/shasum/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjxbyz/shasum.git"
  },
  "license": "MIT",
  "author": "kjxbyz <kjxbyz888@outlook.com> (https://github.com/kjxbyz)",
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "shasum": "bin/shasum.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "test": "vitest run"
  },
  "dependencies": {
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "18.19.39",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "sort-package-json": "^2.10.0",
    "tsup": "^8.1.0",
    "typescript": "^4.9.5",
    "vitest": "^2.0.1"
  },
  "peerDependencies": {
    "glob": "^11.0.0"
  },
  "packageManager": "pnpm@9.4.0",
  "publishConfig": {
    "access": "public"
  }
}
